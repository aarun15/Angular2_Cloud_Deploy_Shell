{
  "name": "sampleApp",
  "version": "1.0.0",
  "description": "An Angular2 project",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "webpackdev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src",
    "rimraf": "rimraf",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "server:dev": "npm run sass && npm run webpackdev",
    "server_aot:dev": "npm run sass && webpack-dev-server --config config/webpack.dev_aot.js --progress --profile --watch --content-base src",
    "server:prod": "http-server dist --cors",
    "server_aot:prod": "http-server \"dist/bundled\" --cors",
    "start": "npm run server:dev",
    "start_aot": "npm run server_aot:dev",
    "sass:watch": "node-sass -w src -o src --include-path node_modules --output-style compressed -q",
    "test": "karma start",
    "clean": "npm run rimraf -- dist",
    "clean:compile": "npm run rimraf -- \"aot\" ",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "webpack_aot:dev": "webpack --config config/webpack.dev_aot.js --progress --profile",
    "build:prod": "npm run sass && npm run clean && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "webpack_aot:prod": "webpack --config config/webpack.prod_aot.js  --progress --profile --bail",
    "webpack_server": "npm run ngc_server && npm run build_express",
    "build_express" : "webpack --config config/webpack.server_prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "build_aot": "npm run build_aot:dev",
    "ngc": "./node_modules/.bin/ngc -p ./tsconfig-aot.json",
    "ngc_server": "./node_modules/.bin/ngc -p ./tsconfig-server.json",
    "start-prod": "npm run ngc && npm run build_aot:prod",
    "sass": "node-sass src -o src --include-path node_modules --output-style compressed -q",
    "build_aot:prod": "npm run sass && npm run clean && npm run clean:compile && npm run ngc && npm run webpack_aot:prod",
    "build_aot:dev": "npm run sass && npm run clean:compile && npm run ngc && npm run webpack_aot:dev",
    "compile:aot": "npm run clean:compile && npm run sass && npm run ngc",
    "start:run": "node dist/bundled/server.bundle.js",
    "start:debug": "npm run nodedebug dist/bundled/server.bundle.js",
     "nodemon": "./node_modules/.bin/nodemon",
    "nodedebug": "./node_modules/.bin/node-debug"
  },
